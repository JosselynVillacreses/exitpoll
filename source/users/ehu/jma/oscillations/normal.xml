  _view.getElement("tx1").reset();
  _view.getElement("tx2").reset();
  _view.getElement("tq1").reset();
  _view.getElement("tq2").reset();
  _view.getElement("tf").reset();
}

public void erasePhase() {
  _view.getElement("x1x2").reset();
  _view.getElement("q1q2").reset();
}


]]></Code>

