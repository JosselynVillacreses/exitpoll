<Code><![CDATA[
y1 = 0;
y2 = 0;
if (x1 < -sl+d || x2 > sl || x2-x1 < -sl+d) {
  _pause();
// _view.getElement("Start").setProperty("image","images/play.gif");
   _alert("bhasi","The simulation is over","At least one spring became too short");
  x1 = x2 = v1 = v2 = 0.0;
  putpos(x1,x2);
}

]]></Code>

</Content>
