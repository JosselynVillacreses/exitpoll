<Osejs.Model.Evolution.Page>
<Type>ODE_EDITOR</Type>
<Name>Evolution</Name>
<Active>true</Active>
<Content>
<IndependentVariable>t</IndependentVariable>
<Increment>dt</Increment>
<Rate state="x1">v1</Rate>
<Rate state="x2">v2</Rate>
<Rate state="v1">-(k*x1+kk*(x1-x2))+f*Math.cos(W*t)-2.*g1*v1</Rate>
<Rate state="v2">-(kk*(x2-x1)+k*x2)-2.*g2*v2</Rate>
<Rate state=""></Rate>
<Method>RungeKuttaFehlberg</Method>
<Tolerance>1e-6</Tolerance>
<Comment><![CDATA[Equations of motion]]></Comment>

</Content>
</Osejs.Model.Evolution.Page>
