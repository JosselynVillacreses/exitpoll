public void setInitial() {
 nPoints2 = nModes;
 nPoints = nPoints2+2;
 t = 0.0;
  A = (ymax-ymin)/(3*nModes);
  for (int i = 0; i < nPoints; i++) {
     x0[i] = 
     x1[i] =
     x2[i] =
     x3[i] =
     x4[i] =
     x5[i] =
     x6[i] =
     x7[i] =
     x8[i] =
     x9[i] =
                (double)i/(double)(nPoints-1);
  }
  for (int n = 0; n < nModes; n++)
     ys[n] = ymin+(3.0*A*n)+1.5*A;
  updateAll();
}
]]></Code>

