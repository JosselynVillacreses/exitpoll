<Type>ODE_EDITOR</Type>
<Name>Dynamical system</Name>
<Active>true</Active>
<Content>
<IndependentVariable>t</IndependentVariable>
<Increment>dt</Increment>
<Rate state="x">vx</Rate>
<Rate state="y">vy</Rate>
<Rate state="vx">(y-x-L)</Rate>
<Rate state="vy">-(y-x-L)/alpha</Rate>
<Rate state=""></Rate>
<Method>Dopri853</Method>
<Tolerance>tol</Tolerance>
<Comment><![CDATA[Equations of motion]]></Comment>
<Events><Event.Page>
<Type>EVENT_EDITOR</Type>
<Name>Collision</Name>
<Active>true</Active>
<EventContent>
<Comment><![CDATA[Collision between internal masses]]></Comment>
<ZeroCondition><![CDATA[
return x;
]]></ZeroCondition>
<Tolerance><![CDATA[tol]]></Tolerance>
<Action><![CDATA[
vx = Math.abs(vx);
ncol++;
_resetSolvers();
if (stopcol) _pause();
computed = true;

]]></Action>
<StopAtEvent>true</StopAtEvent>

</EventContent>
</Event.Page>
<Event.Page>
<Type>EVENT_EDITOR</Type>
<Name>Spring</Name>
<Active>true</Active>
<EventContent>
<Comment><![CDATA[Spring length cannot be negative]]></Comment>
<ZeroCondition><![CDATA[
return y-x;
]]></ZeroCondition>
<Tolerance><![CDATA[0.001]]></Tolerance>
<Action><![CDATA[
_alert("Main","Simulation stopped","Spring too short");
_resetSolvers();
_pause();

]]></Action>
<StopAtEvent>true</StopAtEvent>

</EventContent>
</Event.Page>
<Event.Page>
<Type>EVENT_EDITOR</Type>
<Name>Infinity</Name>
<Active>true</Active>
<EventContent>
<Comment><![CDATA[]]></Comment>
<ZeroCondition><![CDATA[
return xmax+a-x;
]]></ZeroCondition>
<Tolerance><![CDATA[0.001]]></Tolerance>
<Action><![CDATA[
_pause();
]]></Action>
<StopAtEvent>true</StopAtEvent>

</EventContent>
</Event.Page>
</Events>

