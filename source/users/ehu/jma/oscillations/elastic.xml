<Osejs.Model.Evolution>
<Osejs.Model.Evolution.Page>
<Type>ODE_EDITOR</Type>
<Name>Evolution</Name>
<Active>true</Active>
<Content>
<IndependentVariable>t</IndependentVariable>
<Increment>dt</Increment>
<Rate state="r">rd</Rate>
<Rate state="phi">phid</Rate>
<Rate state="rd">r*phid*phid+Math.cos(phi)-k*(r-1.0)</Rate>
<Rate state="phid">-(2.0*rd*phid+Math.sin(phi))/r</Rate>
<Rate state=""></Rate>
<Method>RungeKuttaFehlberg</Method>
<Tolerance>tol</Tolerance>
<Comment><![CDATA[Equations of motion]]></Comment>
<Events><Event.Page>
<Type>EVENT_EDITOR</Type>
<Name>Poincare section</Name>
<Active>true</Active>
<EventContent>
<Comment><![CDATA[]]></Comment>
<ZeroCondition><![CDATA[
return (!showPoincare || nPoincare >= PoincareMax) ? 1.0 : sign*rd;
]]></ZeroCondition>
<Tolerance><![CDATA[epsPoincare]]></Tolerance>
<Action><![CDATA[
sign = -sign;
PoincareX[nPoincare] = r*Math.sin(phi);
PoincareY[nPoincare] = -r*Math.cos(phi);
nPoincare++;
]]></Action>
<StopAtEvent>false</StopAtEvent>

</EventContent>
</Event.Page>
</Events>

</Content>
</Osejs.Model.Evolution.Page>
<Osejs.Model.Evolution.Page>
<Type>EVOLUTION_EDITOR</Type>
<Name>Range</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[Make sure phi is in range]]></Comment>
<Code><![CDATA[
if      (phi < -Math.PI) {
  phi += 2.0*Math.PI;
  _view.getElement("Evolutionphi").setProperty("connected","false");
}
else if (phi >  Math.PI) {
  phi -= 2.0*Math.PI;
  _view.getElement("Evolutionphi").setProperty("connected","false");
}
else
  _view.getElement("Evolutionphi").setProperty("connected","true");
]]></Code>

</Content>
</Osejs.Model.Evolution.Page>
<Osejs.Model.Evolution.Page>
<Type>EVOLUTION_EDITOR</Type>
<Name>Cartesian</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[Cartesian components]]></Comment>
<Code><![CDATA[
double cf = Math.cos(phi),
       sf = Math.sin(phi);  
x = r*sf;
y = -r*cf;
u = rd*sf+r*phid*cf;
v = -rd*cf+r*phid*sf;
]]></Code>

</Content>
</Osejs.Model.Evolution.Page>
</Osejs.Model.Evolution>
