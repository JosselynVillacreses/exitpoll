<Osejs.Model.Initialization>
<Osejs.Model.Initialization.Page>
<Type>CODE_EDITOR</Type>
<Name>Initial values</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[Set initial values in polar coordinates]]></Comment>
<Code><![CDATA[
t     = 
phase = 0.0;
x     = x0;
v     = v0;
xx = (rho+x)*Math.cos(phase);
yy = (rho+x)*Math.sin(phase);
phi = phase0 = Math.toRadians(phaseDegrees);

OX1[0] = Math.cos(phi);		// Axes
OX1[1] = Math.sin(phi);
OX2[0] = Math.cos(phi+Pi2/N);
OX2[1] = Math.sin(phi+Pi2/N);
OX3[0] = Math.cos(phi+2.0*Pi2/N);
OX3[1] = Math.sin(phi+2.0*Pi2/N);
OZ[2] = 1.0;

x1 = rho*OX1[0];			// Centers
y1 = rho*OX1[1];
x2 = rho*OX2[0];
y2 = rho*OX2[1];
x3 = rho*OX3[0];
y3 = rho*OX3[1];

label1 = "\u03C6="+_format(phaseDegrees,"0.##");		//Labels
label2 = "\u03C6="+_format(phaseDegrees+120,"0.##");
label3 = "\u03C6="+_format(phaseDegrees+240,"0.##");

_view.resetTraces();			// Orbit

_resetSolvers();
]]></Code>

</Content>
