<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
public void startCapture() {
  selecting = first = true;
}

public void mouseMove() {
  if (selecting) {
    if (first) {
      wx1 = wx2 = mx2 = mx1 = mx;
      wy1 = wy2 = my2 = my1 = my;
      first = false;
    }
    else {
      mx2 = mx;
      my2 = my;
      wx1 = (mx1 < mx2) ? mx1 : mx2;
      wx2 = (mx1 < mx2) ? mx2 : mx1;
      wy1 = (my1 < my2) ? my1 : my2;
      wy2 = (my1 < my2) ? my2 : my1;
    }
  }
}

public void endCapture() {
  if (!selecting) return;
  selecting = false;
  if (mx1 == mx2 || my1 == my2) return;   
  x1 = wx1;
  x2 = wx2;
  y1 = wy1;
  y2 = wy2;
  clear();
}
]]></Code>

</Content>
