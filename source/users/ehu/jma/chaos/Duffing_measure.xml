if (n <= 0) n = 1;
if (n % 2 == 0) n++;
if  (type.equals("Square") || type.equals("Diskoa")) {
    if (n > nmax) n = nmax;
    npoints = n*n;
    n0 = npoints/2;
}
else if  (type.equals("Hollow")) {
    if (4*n-3 > nmax*nmax) n = (nmax*nmax+3)/4;
    n0 = 4*(n-1);
    npoints = n0+1;
}
else {
    if (2*n-1 > nmax*nmax) n = (nmax*nmax+1)/2;
    n0 = 2*(n-1);
    npoints = n0+1;
}
x0 = Math.sin(q[n0]);
