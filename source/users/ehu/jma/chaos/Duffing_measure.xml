<Content>
<Comment><![CDATA[Set initial coordinates for every point]]></Comment>
<Code><![CDATA[
public void set_type () {
  t = 0.0;
  nPoincare = 0;
  remaining = skip;
  if      (type.equals("Square"))        set_sqdi();
  else if (type.equals("Hollow"))           set_void();
  else if (type.equals("Circle")) set_circ();
}

public void set_sqdi() {       // Full square or disk
  q[n0] = q0;
  qd[n0] = qd0;
  if (n >= 3) {
    int k = n/2;
    double dd = d/(double)k;
     for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            q[i*n+j]  = q0+(i-k)*dd;
            qd[i*n+j] = qd0+(j-k)*dd;
        }
    }
  }
}

public void set_void() {                    // Void square
  q[n0] = q0;
  qd[n0] = qd0;
  if (n >= 3) {
    int k = n/2;
    double dd = d/(double)k;
     for (int i = 0; i < n-1; i++) {
          q[i]  = q0+(i-k)*dd;
          qd[i] = qd0+(n-1-k)*dd;
          q[i+n-1]  = q0+(n-1-k)*dd;
          qd[i+n-1] = qd0-(i-k)*dd;
          q[i+2*(n-1)]  = q0-(i-k)*dd;
          qd[i+2*(n-1)] = qd0-k*dd;
          q[i+3*(n-1)]  = q0-k*dd;
          qd[i+3*(n-1)] = qd0+(i-k)*dd;
    }
  }
}

public void set_circ() {                    // Void disk
  q[n0] = q0;
  qd[n0] = qd0;
  if (n >= 3) {
    double r = d,
               a = Math.PI/(n-1);
     for (int i = 0; i < 2*n-2; i++) {
          q[i]  = q0+r*Math.cos(i*a);
          qd[i] = qd0+r*Math.sin(i*a);
    }
  }
}


]]></Code>

</Content>
