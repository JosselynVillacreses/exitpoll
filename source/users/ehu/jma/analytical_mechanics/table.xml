E0 = 1.5/(r0*r0);
if (E < E0)			// Energia minimoa
  E = E0;                 		

double dd = 0.01;
for (rmin = r0; 0.5/rmin/rmin+Mm*rmin < E; rmin -= dd);
for (rmax = r0; 0.5/rmax/rmax+Mm*rmax < E; rmax += dd);
]]></Code>

