<Code><![CDATA[
public void updateOne(int i,double x[],double y[]) {
  for (int n = 0; n < nPoints; n++) {
     y[n] = ys[i-1]+A*Math.sin(i*PI*x[n])*Math.cos(i*PI*t);
  }
}

public void updateAll() {
  switch (nModes) {
      case 10 : updateOne(10,x9,y9);
      case   9 : updateOne( 9,x8,y8);
      case   8 : updateOne( 8,x7,y7);
      case   7 : updateOne( 7,x6,y6);
      case   6 : updateOne( 6,x5,y5);
      case   5 : updateOne( 5,x4,y4);
      case   4 : updateOne( 4,x3,y3);
      case   3 : updateOne( 3,x2,y2);
      case   2 : updateOne( 2,x1,y1);
      case   1 : updateOne( 1,x0,y0);
  }
}
]]></Code>

</Content>
</Osejs.Model.Library.Page>
<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>Initial</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[Initial values]]></Comment>
<Code><![CDATA[
public void setInitial() {
  t = 0.0;
  A = (ymax-ymin)/(3*nModes);
  for (int i = 0; i < nPoints; i++) {
     x0[i] = 
     x1[i] =
     x2[i] =
     x3[i] =
     x4[i] =
     x5[i] =
     x6[i] =
     x7[i] =
     x8[i] =
     x9[i] =
                (double)i/(double)(nPoints-1);
  }
  for (int n = 0; n < nModes; n++)
     ys[n] = ymin+(3.0*A*n)+1.5*A;
  updateAll();
}
]]></Code>

</Content>
