public void setColors() {
  org.opensourcephysics.ejs.control.swing.ControlDrawable drawable =
   (org.opensourcephysics.ejs.control.swing.ControlDrawable)
    _view.getElement("Modes");

  org.opensourcephysics.display2d.DataRaster raster =
   (org.opensourcephysics.display2d.DataRaster) drawable.getDrawable();

  raster.setColor(0, new java.awt.Color(0,0,0));	// No color = black

  for (int n = 0; n < N; n++)
    raster.setColor(10+n,new java.awt.Color(java.awt.Color.HSBtoRGB(1.0f/N*n,1.0f,1.0f)));
}

]]></Code>

