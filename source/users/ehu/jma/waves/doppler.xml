<Osejs.Model.Initialization>
<Osejs.Model.Initialization.Page>
<Type>CODE_EDITOR</Type>
<Name>Start</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[Set initial values]]></Comment>
<Code><![CDATA[
t = 0.0;
nDetected = 0;
nEmitted = 1;
for(int i = 0; i < n; i++){
 x0[i]        =
 y0[i]        =
 d[i]         =
 vd[i]        = 
 tEmitted [i] =
 tDetected[i] = 0.0;
 bDetected[i] = false;
}
vd[0] = 2.0*c;
]]></Code>

</Content>
</Osejs.Model.Initialization.Page>
