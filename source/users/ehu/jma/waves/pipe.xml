if (n < 1) n = 1;			// Mode

]]></Code>

</Content>
</Osejs.Model.Constraints.Page>
<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>Nodes</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[Node positions]]></Comment>
<Code><![CDATA[
for (int i = 1; i < 6; i++)
    xnode[i-1] = (leftEnd) ? ((rightEnd) ? i/(double)n : i/(n-.5)) 
                           : ((rightEnd) ? (2*i-1)/(2*n-1.) : (2*i-1)/(double)(2*n));
]]></Code>

</Content>
</Osejs.Model.Constraints.Page>
<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>Update upper</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[Set values for upper display]]></Comment>
<Code><![CDATA[
double dx = xmax/nField;
for (int i = 0; i <= nField; i++) {
  uField[i] = .5+A*((leftEnd) ? ((rightEnd) ? Math.sin(n*pi*xField[i]) 
                                            : Math.sin((n-.5)*pi*xField[i])) 
                              : ((rightEnd) ? Math.cos((n-.5)*pi*xField[i]) 
                                            : Math.cos(n*pi*xField[i]))    )*
                    Math.cos(omega*t);
  pField[i] = .5+A*((leftEnd) ? ((rightEnd) ? -Math.cos(n*pi*xField[i]) 
                                            : -Math.cos((n-.5)*pi*xField[i])) 
                              : ((rightEnd) ? Math.sin((n-.5)*pi*xField[i]) 
                                            : Math.sin(n*pi*xField[i]))    )*
                    Math.cos(omega*t);
}
]]></Code>

</Content>
</Osejs.Model.Constraints.Page>
<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>Displacements</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[Update displacements]]></Comment>
<Code><![CDATA[
double dx = xmax/nDisp;
for (int i = 0; i <= nDisp; i++) {
  uDisp[i] = xDisp[i]+
             Adisp*((leftEnd) ? ((rightEnd) ? Math.sin(n*pi*xDisp[i]) 
                                            : Math.sin((n-.5)*pi*xDisp[i])) 
                              : ((rightEnd) ? Math.cos((n-.5)*pi*xDisp[i]) 
                                            : Math.cos(n*pi*xDisp[i]))    )*
                    Math.cos(omega*t);
}
]]></Code>

</Content>
</Osejs.Model.Constraints.Page>
<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>Pressure</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[Set current values for pressure]]></Comment>
<Code><![CDATA[
double dx = xmax/nx,
       dy = 1.0/ny;
for (int i = 0; i <= nx; i++) {
   double x = i*dx;
   for (int j = 0; j <= ny; j++) {
     double y = ((leftEnd) ? ((rightEnd) ? -Math.cos(n*pi*x) 
                                         : -Math.cos((n-.5)*pi*x)) 
                           : ((rightEnd) ? Math.sin((n-.5)*pi*x) 
                                         : Math.sin(n*pi*x))    );
     space[i][j] = y*Math.cos(omega*t);
  } 
}
]]></Code>

</Content>
</Osejs.Model.Constraints.Page>
