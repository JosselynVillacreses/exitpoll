<Code><![CDATA[
void computeForce(double[] state) {
  System.arraycopy(zeros,0,force,0,force.length);
  for(int i=0; i<nBodies; i++){
    for(int j=i+1; j<nBodies; j++){
      double dx = state[4 * i] - state[4 * j];
      double dy = state[4 * i+2] - state[4 * j+2];
      double r2=dx*dx+dy*dy;
      double r3=r2*Math.sqrt(r2);
      double fx=dx/r3;
      double fy=dy/r3;
      force[2*i] -= fx;
      force[2*i + 1] -= fy;
      force[2*j] += fx;
      force[2*j + 1] += fy;
    }
  }
}

public double[] acc (double state[]) {
  computeForce(state); // force array alternates fx and fy
  for (int i=0,iindex=0; i<nBodies; i++,iindex+=4) {   
    rate[iindex  ] = state[iindex+1];  // x rate is vx
    rate[iindex+1] = force[2*i];   // vx rate is fx
    rate[iindex+2] = state[iindex+3];  // y rate is vy
    rate[iindex+3] = force[2*i+1]; // vy rate is fy
  }
  return rate;
}

]]></Code>

