<Type>LIBRARY_EDITOR</Type>
<Name>Potential Energy</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
public double getPE() {
  double pe=0;
  double cut2 = 3.0*3.0;
  for(int i = 0, n=x.length; i < n-1; i++)
    for(int j = i+1; j < n; j++) {
     double dx = separation(x[i] - x[j], cellSize);
     double dy = separation(y[i] - y[j], cellSize);
     double r2 = dx*dx + dy*dy;
     if(r2 < cut2) {
       double oneOverR2 = 1.0/r2;
       double oneOverR6 = oneOverR2*oneOverR2*oneOverR2;
       pe += 4.0*epsilon*(oneOverR6*oneOverR6-oneOverR6);
     }
   }
  return pe;
}

public double potentialEnergyChange (int i, double xtrial, double ytrial) {
  double cut2 = 3.0*3.0;
  double pe=0;
  for(int j = 0, n=x.length; j < n; j++){
     if(i==j) continue;
     double dx = separation(x[i] - x[j], cellSize);
     double dy = separation(y[i] - y[j], cellSize);
     double r2 = dx*dx + dy*dy;
     if(r2 < cut2) {
       double oneOverR2 = 1.0/r2;
       double oneOverR6 = oneOverR2*oneOverR2*oneOverR2;
       pe += 4.0*epsilon*(oneOverR6*oneOverR6-oneOverR6);
     }
   }
  double peTrial=0;
  for(int j = 0, n=x.length; j < n; j++){
     if(i==j) continue;
     double dx = separation(xtrial - x[j], cellSize);
     double dy = separation(ytrial - y[j], cellSize);
     double r2 = dx*dx + dy*dy;
     if(r2 < cut2) {
       double oneOverR2 = 1.0/r2;
       double oneOverR6 = oneOverR2*oneOverR2*oneOverR2;
       peTrial += 4.0*epsilon*(oneOverR6*oneOverR6-oneOverR6);
     }
  }
  return peTrial-pe;
}
]]></Code>

</Content>
</Osejs.Model.Library.Page>
<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>Periodic Boundary Conditions</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
// Adjusts the postion of a particle assuming peridoic boundary conditions.
// The postion will be in the interval [0,size).
public double position(final double r, final double size) {
    return r<0 ? r%size+size : r%size;
}

//Computes the minimum separation using periodic boundary conditions.
public double separation(final double dr, final double size) {
    return dr-size*Math.floor(dr/size+0.5);
}
]]></Code>

</Content>
</Osejs.Model.Library.Page>
