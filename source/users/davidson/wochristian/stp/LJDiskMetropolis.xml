<Osejs.Model.Initialization>
<Osejs.Model.Initialization.Page>
<Type>CODE_EDITOR</Type>
<Name>Initialize Particles</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
int n=x.length;
double separation=cellSize/Math.sqrt(n);
double xi=separation/2;
double yi=separation/2;
double initialSpeed=Math.sqrt(2*temperature);
// set the velocity
for(int i=0; i<n; i++){
  x[i]=xi;
  y[i]=yi;
  double angle = 2*Math.PI*Math.random();
  vx[i]=initialSpeed*Math.sin(angle);
  vy[i]=initialSpeed*Math.cos(angle);
  speed[i]=initialSpeed;
  xi += separation;
  if(xi+diameter>cellSize){
   xi = diameter;
   yi += separation;
  }
}
totalKineticEnergy = n*initialSpeed*initialSpeed/2;
totalPotentialEnergy= getPE(); 
]]></Code>

</Content>
</Osejs.Model.Initialization.Page>
</Osejs.Model.Initialization>
<Osejs.Model.Evolution>
<Osejs.Model.Evolution.Page>
<Type>EVOLUTION_EDITOR</Type>
<Name>Metropolis Algorithm</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
for(int j=0, n=x.length; j<n; j++){
  int i=(int)(n*Math.random()); // choose a particle at random
  double xtrial = x[i]+(2*Math.random()-1.0)*maxDr;
  double ytrial = y[i]+(2*Math.random()-1.0)*maxDr;
  double vxtrial = vx[i]+(2*Math.random()-1.0)*maxDv;
  double vytrial = vy[i]+(2*Math.random()-1.0)*maxDv;
  double dKE= 0.5*(vxtrial*vxtrial+vytrial*vytrial- speed[i]*speed[i]);
  double dE = dKE + potentialEnergyChange (i, xtrial,ytrial);
  if( (dE<0) || (Math.exp(-beta*dE)>Math.random())){
    accepted++;
    x[i]=position(xtrial, cellSize);;
    y[i]=position(ytrial, cellSize);
    vx[i]=vxtrial;
    vy[i]=vytrial;
    speed[i]=Math.sqrt(vxtrial*vxtrial+vytrial*vytrial);
  }
}
mcsteps++;
]]></Code>

</Content>
</Osejs.Model.Evolution.Page>
</Osejs.Model.Evolution>
<Osejs.Model.Constraints>
<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>Compute Speeds and Energy</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
totalKineticEnergy=0;
for(int i=0; i<n; i++){
  double v2=vx[i]*vx[i]+vy[i]*vy[i];
  totalKineticEnergy += v2;
  speed[i]=Math.sqrt(v2);
}
totalPotentialEnergy = getPE(); 
totalKineticEnergy = 0.5*totalKineticEnergy;
if(!accumulateData) _view.speedHistogram.clear();
]]></Code>

</Content>
</Osejs.Model.Constraints.Page>
</Osejs.Model.Constraints>
<Osejs.Model.Library>
