public void selectHistogram() {
  if(speedHistogram){
    histogramInput=speed;
    histogramBinLabel="speed";
  }else{
   histogramInput=vx;
    histogramBinLabel="v_x";
  }
  _view.speedHistogram.clear();
}
]]></Code>

