<Type>LIBRARY_EDITOR</Type>
<Name>getA1</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
public double getA1 (double c1, double c2,double w1,double w2) {
  return  -gravity*((m1+m2)*Math.sin(c1)-m2*Math.sin(c2)*Math.cos(c1-c2))/((m1+m2)-m2*Math.cos(c1-c2)*Math.cos(c1-c2))/L1;
}
public double getA2 (double c1,double c2,double w1,double w2) {
  return -gravity*(m1+m2)*(Math.sin(c1)*Math.cos(c1-c2)-Math.sin(c2))/(m2*Math.cos(c1-c2)*Math.cos(c1-c2)-(m1+m2))/L2;
}
]]></Code>

</Content>
</Osejs.Model.Library.Page>
<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>setPendulum</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
public void setPendulum () {
 y1=ya-ymax;
 x2=xb-x1;
 y2=yb-ya;
 L1=Math.sqrt(x1*x1+y1*y1);
 L2=Math.sqrt(x2*x2+y2*y2);
 cta1=Math.atan2(x1,-y1);
 cta2=Math.atan2(x2,-y2);
 cst1=-gravity/L1;
 cst2=-gravity/L2;
 cst12=-m2*gravity/(m1*L1);
 omega1=0.;
 omega2=0.;
}
]]></Code>

</Content>
</Osejs.Model.Library.Page>
