<Name>Plot Wave Function</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
double dx = (xmax-xmin)/npts,x=xmin+dx/2;
_view.waveFunctionTrace.clear();
for(int i=0; i<npts; i++){
  double u = _view.waveFunction.evaluate(x);
  _view.waveFunctionTrace.addPoint(x,u);
  x+=dx;
}
]]></Code>

